CRE=1000

F-IN=FILEDTCR.PIN1
F-OT=FILEDTCR.POT1

CSV=N

SORT=AAA

 KEY=1,POS=81,LEN=10,TYPE=ZD
 KEY=2,POS=91,LEN=10,TYPE=ZD
 KEY=3,POS=1,LEN=9,TYPE=ZD

 DT=01,POS=001,LEN=9,TYPE=NUM,IDX=1,FROM=1,TO=100000,SIGN=1,ZERO=N
 DT=02,POS=011,LEN=9,TYPE=NUM,IDX=2,FROM=1000,TO=2000,SIGN=1,ZERO=Y
 DT=03,POS=021,LEN=9,TYPE=NUM,IDX=3,FROM=1000,TO=2000,SIGN=1,ZERO=N
 DT=04,POS=031,LEN=9,TYPE=NUM,IDX=4,FROM=1000,TO=2000,SIGN=1,ZERO=Y
 DT=05,POS=041,LEN=9,TYPE=NUM,IDX=5,FROM=1000,TO=2000,SIGN=1,ZERO=N
 DT=06,POS=051,LEN=9,TYPE=NUM,IDX=6,FROM=1000,TO=2000,SIGN=1,ZERO=Y
 DT=07,POS=061,LEN=9,TYPE=NUM,IDX=7,FROM=1000,TO=2000,SIGN=1,ZERO=N
 DT=08,POS=071,LEN=9,TYPE=NUM,IDX=8,FROM=1000,TO=2000,SIGN=1,ZERO=Y
 DT=09,POS=081,LEN=9,TYPE=NUM,IDX=9,FROM=1000,TO=2000,SIGN=1,ZERO=N
 DT=10,POS=091,LEN=9,TYPE=NUM,IDX=10,FROM=1000,TO=2000,SIGN=1,ZERO=Y

 DT=01,POS=001,LEN=9,TYPE=NUM,IDX=1,FROM=1000,TO=2000,SIGN=1,ZERO=

 DT=01,POS=001,LEN=9,TYPE=CODE,IDX=1,FROM=1000,TO=1,BETWEEN=1 
 DT=02,POS=011,LEN=9,TYPE=CODE,IDX=2,FROM=2000,TO=2,BETWEEN=2
 DT=03,POS=021,LEN=9,TYPE=CODE,IDX=3,FROM=3000,TO=3,BETWEEN=3
 DT=04,POS=031,LEN=9,TYPE=CODE,IDX=4,FROM=4000,TO=4,BETWEEN=4
 DT=05,POS=041,LEN=9,TYPE=CODE,IDX=5,FROM=5000,TO=5,BETWEEN=5
 DT=06,POS=051,LEN=9,TYPE=CODE,IDX=6,FROM=6000,TO=6,BETWEEN=6
 DT=07,POS=061,LEN=9,TYPE=CODE,IDX=7,FROM=7000,TO=7,BETWEEN=7
 DT=08,POS=071,LEN=9,TYPE=CODE,IDX=8,FROM=8000,TO=8,BETWEEN=8
